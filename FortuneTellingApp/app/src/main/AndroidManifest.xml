<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fortunetellingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Fragment.CompatibilityFragment"></activity>
        <activity android:name=".Fragment.DetailFragment" />
        <activity android:name=".Fragment.ResultFragment" />
        <activity android:name=".Fragment.MyFortuneFragment" />
        <activity android:name=".Fragment.AccountFragment" />
        <activity android:name=".Fragment.SNSFragment" />
        <activity android:name=".Fragment.SearchFragment" />
        <activity android:name=".Fragment.HomeFragment" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>